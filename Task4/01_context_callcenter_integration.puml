```plantuml
@startuml
!includeurl https://raw.githubusercontent.com/plantuml-stdlib/C4-PlantUML/master/C4_Context.puml

title Контекст: Интеграция кол-центров с системой ставок

Person(клиент, "Клиент", "Звонит для консультации по депозитам")
Person(менеджер_банк, "Менеджер кол-центра банка", "Консультирует клиентов")
Person(менеджер_партнер, "Менеджер партнёрского кол-центра", "Консультирует клиентов")
Person(администратор, "Администратор бэк-офиса", "Управляет выгрузкой ставок")

System(колцентр_банк, "Система кол-центра банка", "Java, React.js, PostgreSQL")
System_Ext(колцентр_партнер, "Система партнёрского кол-центра", "Внешняя система")
System(product_service, "Product Service", "Java Spring Boot", "Управление ставками")

System_Ext(sftp_сервер, "SFTP сервер", "Сервер для обмена файлами")

Rel(клиент, менеджер_банк, "Звонит для консультации", "Телефон")
Rel(клиент, менеджер_партнер, "Звонит для консультации", "Телефон")

Rel(менеджер_банк, колцентр_банк, "Работает с системой", "Web UI")
Rel(менеджер_партнер, колцентр_партнер, "Работает с системой", "UI")

Rel(колцентр_банк, product_service, "Запрашивает актуальные ставки", "REST/HTTPS\n< 500 мс")

Rel(администратор, product_service, "Запускает выгрузку", "Web UI")
Rel(product_service, sftp_сервер, "Выгружает файлы ставок", "SFTP")
Rel(колцентр_партнер, sftp_сервер, "Забирает файлы ставок", "SFTP")

@enduml